        "analytics",
        "transactions",
        "auditing",
        "insights",
        "compliance"
    ],
    "homepage": "https://doc.jobmetric.net/package/numi",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
