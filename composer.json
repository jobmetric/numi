{
    "name": "jobmetric/numi",
    "description": "An intuitive accounting management panel designed to simplify financial tracking and reporting, empowering businesses to manage their finances with precision and ease.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
